#include "registryinfov2.rh"

RESOURCE REGISTRY_INFO theInfo
	{
	resource_format_version = RESOURCE_FORMAT_VERSION_2;
	// UID for the DLL
	dll_uid = 0x102010DA;
	// Declare array of interface info
	interfaces = 
		{
		INTERFACE_INFO
			{
			// UID of interface that is implemented
			interface_uid = 0x102010D8;
			implementations = 
				{
				IMPLEMENTATION_INFO
					{
					implementation_uid = 0x102010DA;
					version_no = 1;
					// This must map to the "IAP\IAPServiceType" field in CommDb
					default_data = "LANService";
					rom_only = 1;
					}
				};
			}
		};
	}
